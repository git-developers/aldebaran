<div class="col-md-{{ element.numberOfColumns }} div-element div-element-{{ element.id }}" data-id="{{ element.id }}">
    <div class="box box-primary" >

        <div class="box-header with-border {{ element.boxHeaderClass }}" style="{{ element.boxHeaderStyle }}" >
            <h2 class="box-title " style="font-size: 15px">
                <button type="button" class="btn bg-default btn-xs">{{ element.id }}</button> {{ element.Label | raw }}
            </h2>
            <div class="box-tools pull-right">
                <button type="button" data-id="{{ element.id }}" class="btn btn-box-tool edit">
                    <i class="fa fa-fw fa-pencil-square-o"></i>
                </button>
                <button type="button" data-id="{{ element.id }}" class="btn btn-box-tool delete"> {#data-widget="remove"#}
                    <i class="fa fa-remove"></i>
                </button>
            </div>
        </div>

        <div class="box-body" style="{{ element.boxBodyStyle }}" >
            <div class="form-group" style="margin: 0 3px 0 3px">
                {{ element.Tag | raw }}
            </div>
        </div>

    </div>
</div>

